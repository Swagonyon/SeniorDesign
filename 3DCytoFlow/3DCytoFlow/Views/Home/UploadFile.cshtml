
@{
    ViewBag.Title = "UploadFile";
}

<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Upload Data</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>

    <div class="form-group" id="validator">

        <input type="file" id="selectFile" name="fileupload" />
        <br>
        <input type="submit" class="btn btn-default" name="fileUpload" id="fileUpload" value="Submit" />
        <br>
        <br>
        <div class="progress">
            <div id="progressBar" class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 0%;">
                <span class="sr-only"></span>
            </div>
        </div>
    </div>

    <label id="statusMessage"></label>

    @section Scripts{
        <script src="~/Scripts/chunked-uploader.js"></script>
    }
</div>
